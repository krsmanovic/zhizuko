{
    "$schema": "./appsettings-schema.json",
    "Serilog": {
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "Microsoft.Hosting.Lifetime": "Information",
          "System": "Warning"
        }
      }
    },
    "RobotsTxtOption": "Cms",
    "Umbraco": {
      "CMS": {
        "Hosting": {
          "Debug": true
        },
        "Global": {
          "Id": "b396be80-3d27-48f4-b69d-8e9170ba0a5c",
          "SanitizeTinyMce": true
        },
        "Content": {
          "AllowEditInvariantFromNonDefault": true,
          "ContentVersionCleanupPolicy": {
            "EnableCleanup": true
          },
          "Error404Collection": [
              {
                  "Culture": "sr-Latn-RS",
                  "ContentKey": "f1a5cce1-8c8f-4866-bfe9-e2a0cf63d07a"
              },
              {
                "Culture": "en-US",
                "ContentKey": "f1a5cce1-8c8f-4866-bfe9-e2a0cf63d07a"
              },
              {
                "Culture": "default",
                "ContentKey": "f1a5cce1-8c8f-4866-bfe9-e2a0cf63d07a"
              }
          ]
        },      
        "ModelsBuilder": {
          "ModelsMode": "SourceCodeManual",
          "ModelsNamespace": "GradimoZajedno.Models.Generated",
          "FlagOutOfDateModels": false,
          "ModelsDirectory": "~/../GradimoZajedno.Models/Generated",
          "AcceptUnsafeModelsDirectory": true,
          "DebugLevel": 0
        }
      }
    },
    "ConnectionStrings": {
      "umbracoDbDSN": "Server=localhost;Database=${umbraco_db_name};User Id=GradimoZajedno;Password=${zhizuko_db_password};",
      "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
    },
    "SmtpSettings": {
      "Username": "${smtp_username}",
      "Host": "smtp.gmail.com",
      "Port": 587,
      "To": "${smtp_to}",
      "Password": "${smtp_password}"
    }
  }
  